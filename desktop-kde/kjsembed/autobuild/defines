PKGNAME=kjsembed
PKGSEC=kde
PKGDEP="ki18n kjs"
BUILDDEP="extra-cmake-modules kdoctools"
PKGDES="An easy-to-use wrapper around the KDE ECMAScript interpreter"

CMAKE_AFTER="-DBUILD_COVERAGE=OFF \
             -DBUILD_QCH=ON \
             -DBUILD_TESTING=OFF \
             -DBUILD_WITH_QT6=OFF \
             -DKDE_INSTALL_PREFIX_SCRIPT=OFF \
             -DKDE_INSTALL_USE_QT_SYS_PATHS=ON"
