VER=8u101b13
REL=1
if [[ "${CROSS:-$ARCH}" = "arm64" ]]; then
	SRCTBL="https://repo.aosc.io/misc/aarch64-java/openjdk-$VER.tar.xz"
elif [[ "${CROSS:-$ARCH}" = "armel" ]]; then
	SRCTBL="https://repo.aosc.io/misc/aarch32-java/openjdk-$VER.tar.xz"
else
	SRCTBL="https://repo.aosc.io/misc/openjdk-$VER.tar.xz"
fi
