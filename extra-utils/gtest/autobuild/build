cmake -DBUILD_SHARED_LIBS=ON -DCMAKE_SKIP_RPATH=ON .
make 
mkdir -pm 0755 `pwd`/abdist/usr/{lib,include/gtest/internal,share/licenses/gtest,src/gtest/src,src/gtest/cmake}
install -m 0644 libgtest{,_main}.so `pwd`/abdist/usr/lib
install -m 0644 include/gtest/*.h `pwd`/abdist/usr/include/gtest
install -m 0644 include/gtest/internal/*.h `pwd`/abdist/usr/include/gtest/internal/
install -m 0644 fused-src/gtest/* `pwd`/abdist/usr/src/gtest/src/
install -m 0644 CMakeLists.txt `pwd`/abdist/usr/src/gtest/
install -m 0644 cmake/* `pwd`/abdist/usr/src/gtest/cmake/
