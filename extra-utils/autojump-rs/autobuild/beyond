abinfo "Extracting assets ..."
mkdir -pv "$SRCDIR"/../assets/
tar xvf "$SRCDIR"/../assets.tar.gz --directory "$SRCDIR"/../assets

abinfo "Installing assets ..."
mkdir -pv "$PKGDIR"/etc/bashrc.d
install -Dvm644 "$SRCDIR"/../assets/autojump-release-*/bin/autojump.bash "$PKGDIR"/etc/bashrc.d/40-autojump-rs

abwarn "Assets for zsh and fish are not installed!"
