PKGNAME=usbutils
PKGDES="A utility used to display information about USB buses in the system and the devices connected to them"
PKGDEP="libusb hwdata python-3 coreutils zlib"
PKGDEP__RETRO="libusb hwdata coreutils zlib"
PKGDEP__ARMV4="${PKGDEP__RETRO}"
PKGDEP__ARMV6HF="${PKGDEP__RETRO}"
PKGDEP__ARMV7HF="${PKGDEP__RETRO}"
PKGDEP__I486="${PKGDEP__RETRO}"
PKGDEP__LOONGSON2F="${PKGDEP__RETRO}"
PKGDEP__POWERPC="${PKGDEP__RETRO}"
PKGDEP__PPC64="${PKGDEP__RETRO}"
PKGSEC=utils

AUTOTOOLS_AFTER="--datadir=/usr/share/hwdata"
RECONF=0

PKGPROV="lsusb"
