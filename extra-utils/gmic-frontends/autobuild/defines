PKGNAME=gmic-frontends
PKGSEC=utils
PKGDEP="gmic qt-5"
BUILDDEP="gimp"
PKGDES="QT frontend and gimp plugin for GMIC"

__CMAKE_AFTER_QT="
	-DENABLE_DYNAMIC_LINKING=ON
	-DGMIC_PATH=$SRCDIR/src
	-DGMIC_QT_HOST=none
"
__CMAKE_AFTER_GIMP="
	-DENABLE_DYNAMIC_LINKING=ON
	-DGMIC_PATH=$SRCDIR/src
	-DGMIC_QT_HOST=gimp
"
