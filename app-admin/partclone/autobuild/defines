PKGNAME=partclone
PKGSEC=utils
PKGDEP="libreiserfs ntfs-3g"
PKGDES="Utilities to save and restore used blocks on a partition"

AUTOTOOLS_AFTER="--enable-extfs \
                 --enable-xfs \
                 --enable-reiserfs \
                 --disable-reiser4 \
                 --enable-hfsp \
                 --enable-fat \
                 --enable-exfat \
                 --enable-f2fs \
                 --disable-nilfs2 \
                 --enable-ntfs \
                 --disable-ufs \
                 --disable-vmfs \
                 --disable-jfs \
                 --enable-btrfs \
                 --enable-minix \
                 --enable-ncursesw \
                 --enable-fs-test"
ABSHADOW=0
