PKGNAME=efivar
PKGSEC=admin
PKGDEP="popt"
BUILDDEP="mandoc"
PKGDES="Tools and library for manipulating EFI variables"

# Fedora: This package implements symbol versioning with toplevel ASM
# statments which is incompatible with LTO. Disable LTO.
NOLTO=1

