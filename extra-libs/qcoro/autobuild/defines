PKGNAME=qcoro
PKGSEC=libs
PKGDES="A C++ coroutines library for Qt"
PKGDEP="qt-5"

CMAKE_AFTER="-DUSE_QT_VERSION=5 \
             -DBUILD_TESTING=OFF \
             -DBUILD_SHARED_LIBS=ON \
             -DQCORO_ENABLE_ASAN=OFF \
             -DQCORO_WITH_QTDBUS=ON \
             -DQCORO_WITH_QTNETWORK=ON"
