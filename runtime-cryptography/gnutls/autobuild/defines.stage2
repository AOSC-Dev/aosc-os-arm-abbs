PKGNAME=gnutls
PKGDES="Library that provides a secure layer over a reliable transport layer"
PKGSEC=libs

PKGDEP="gmp nettle readline libtasn1"
BUILDDEP="gperf"

ABTYPE=autotools
AUTOTOOLS_AFTER="--without-idn --without-p11-kit \
                 --enable-local-libopts --disable-libopts-install \
                 --disable-libdane --disable-tools"

AB_FLAGS_O3=1

RECONF=0
