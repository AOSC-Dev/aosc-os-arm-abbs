abinfo "Installing icons ..."
for size in 16x16 32x32 48x48 64x64 128x128 256x256 512x512 ; do
	install -Dvm644 "$SRCDIR"/resources/icons/$size.png \
		"$PKGDIR"/usr/share/icons/hicolor/$size/apps/lx-music-desktop.png
done
install -Dvm644 "$SRCDIR"/resources/icons/icon.ico \
	"$PKGDIR"/usr/share/lx-music-desktop/favicon.ico
