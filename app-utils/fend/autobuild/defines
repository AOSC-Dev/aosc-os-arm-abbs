PKGNAME="fend"
PKGDES="A multi-function command-line calculator"
PKGDEP="glibc gcc-runtime"

PKGDEP_TIER2="$PKGDEP openssl"
PKGDEP__LOONGSON3="$PKGDEP_TIER2"
PKGDEP__PPC64EL="$PKGDEP_TIER2"
PKGDEP__RISCV64="$PKGDEP_TIER2"

BUILDDEP="rustc llvm"
PKGSEC="utils"

CARGO_AFTER_TIER2="-p fend --no-default-features --features native-tls"
CARGO_AFTER__LOONGSON3="$CARGO_AFTER_TIER2"
CARGO_AFTER__RISCV64="$CARGO_AFTER_TIER2"
CARGO_AFTER__PPC64EL="$CARGO_AFTER_TIER2"

USECLANG=1
ABSPLITDBG=0
