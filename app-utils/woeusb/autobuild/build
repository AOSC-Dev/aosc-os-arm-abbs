abinfo "Installing WoeUSB ..."
install -Dvm755 "$SRCDIR/sbin/woeusb" "$PKGDIR/usr/bin/woeusb"

abinfo "Installing WoeUSB manpages ..."
install -Dvm644 "$SRCDIR/share/man/man1/woeusb.1" \
	"$PKGDIR/usr/share/man/man1/woeusb.1"

abinfo "Installing WoeUSB icons ..."
install -Dvm644 "$SRCDIR/share/woeusb/woeusb.svg" \
	"$PKGDIR/usr/share/icons/hicolor/scalable/apps/woeusb.svg"
