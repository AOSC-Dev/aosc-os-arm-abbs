PKGNAME="dua"
PKGDES="A tool to conveniently learn about the usage of disk space of a given directory"
PKGDEP="glibc gcc-runtime"
BUILDDEP="llvm rustc"
PKGSEC="utils"
PKGPROV="dua-cli"

USECLANG=1

# FIXME: loongson3 and mips64r6el has no lld linker support
NOLTO__LOONGSON3=1
USECLANG__LOONGSON3=0
NOLTO__MIPS64R6EL=1
USECLANG__MIPS64R6EL=0
