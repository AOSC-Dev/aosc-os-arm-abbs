PKGNAME=mtr
PKGSEC=net
PKGDEP="jansson ncurses"
PKGDES="A network diagnostic tool that combines the functionality of traceroute and ping"

AUTOTOOLS_AFTER="--enable-ipv6 \
                 --enable-bash-completion \
                 --without-libasan \
                 --without-gtk \
                 --with-jansson \
                 --with-ncurses \
                 --with-ipinfo"
