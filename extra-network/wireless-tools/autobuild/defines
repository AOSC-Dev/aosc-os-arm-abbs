PKGNAME=wireless-tools
PKGSEC=net
PKGDEP="glibc"
PKGDES="Tools allowing to manipulate the Wireless Extensions"

MAKE_AFTER="INSTALL_DIR=$PKGDIR/usr/bin \
            INSTALL_LIB=$PKGDIR/usr/lib \
            INSTALL_INC=$PKGDIR/usr/include \
            INSTALL_MAN=$PKGDIR/usr/share/man \
            BUILD_STRIPPING=n"

NOPARALLEL=1
