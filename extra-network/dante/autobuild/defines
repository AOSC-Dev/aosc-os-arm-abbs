PKGNAME=dante
PKGSEC=net
PKGDEP="linux-pam miniupnpc krb5 tcp-wrappers cyrus-sasl openldap"
PKGDES="SOCKS v4 and v5 compatible proxy server and client"

AUTOTOOLS_AFTER="--enable-drt-fallback \
  --with-gssapi \
  --with-krb5 \
  --with-sasl \
  --with-ldap \
  --with-upnp \
  --with-libwrap \
  --with-pam"
ABSHADOW=no
