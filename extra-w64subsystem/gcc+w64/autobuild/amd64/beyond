ln -sv x86_64-w64-mingw32-gcc $PKGDIR/opt/w64/bin/x86_64-w64-mingw32-cc
ln -sv i686-w64-mingw32-gcc $PKGDIR/opt/w64/bin/i686-w64-mingw32-cc
mkdir $PKGDIR/opt/w64/{x86_64,i686}-w64-mingw32/bin/
mv $PKGDIR/opt/w64/x86_64-w64-mingw32/lib/*.dll $PKGDIR/opt/w64/x86_64-w64-mingw32/bin/
mv $PKGDIR/opt/w64/i686-w64-mingw32/lib/*.dll $PKGDIR/opt/w64/i686-w64-mingw32/bin/
rm -rf $PKGDIR/opt/w64/share/info/dir
