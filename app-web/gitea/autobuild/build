abinfo "Building Gitea ..."
# FIXME: flag provided but not defined: -Wl,-O1,--sort-common,--as-needed
unset LDFLAGS
TAGS="bindata timetzdata sqlite sqlite_unlock_notify" make build

abinfo "Installing Gitea ..."
install -Dvm755 "$SRCDIR"/gitea "$PKGDIR"/usr/bin/gitea
install -Dvm644 "$SRCDIR"/custom/conf/app.example.ini "$PKGDIR"/etc/gitea/app.ini
