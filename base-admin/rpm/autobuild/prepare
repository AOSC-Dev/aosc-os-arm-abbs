export PYTHON="/usr/bin/python3"
export CPPFLAGS="${CPPFLAGS} -I/usr/include/nspr -I/usr/include/nss" \
export CFLAGS="${CFLAGS} -I/usr/include/nspr -I/usr/include/nss -lpython3.4m" \
export LDFLAGS="${LDFLAGS} -lpython3.4m"
export CXXFLAGS="${CXXFLAGS} -I/usr/include/nspr -I/usr/include/nss -lpython3.4m"
