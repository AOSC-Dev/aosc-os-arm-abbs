PKGNAME=nautilus
PKGSEC=gnome
PKGDEP="libexif gnome-desktop exempi gvfs desktop-file-utils dconf tracker libzeitgeist libnotify packagekit"
BUILDDEP="gnome-commom gtk-doc"
PKGDES="GNOME file manager"

AUTOTOOLS_AFTER="--libexecdir=/usr/lib/nautilus \
                 --disable-update-mimedb \
                 --enable-packagekit \
                 --disable-schemas-compile \
                 --disable-appindicator \
                 --disable-unity \
                 --enable-gtk-doc"
