PKGNAME=kstreamripper-trinity
PKGSEC=Trinity
PKGDEP="streamripper tdelibs"
BUILDDEP="tde-common-cmake"
PKGDES="A streamripper frontend for the Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DBUILD_ALL=ON \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF"

PKGCONFL="kstreamripper"

NOLIBTOOL=0
