PKGNAME=libkipi-trinity
PKGSEC=Trinity
PKGDEP="tdelibs tqtinterface"
BUILDDEP="tde-common-cmake"
PKGDES="A image plugin interface library for the Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DBUILD_ALL=ON \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF"

PKGCONFL="libkipi"

NOLIBTOOL=0
