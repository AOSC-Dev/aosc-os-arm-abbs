PKGNAME=libart-lgpl
PKGSEC=libs
PKGDEP="glibc"
BUILDDEP="tde-common-cmake"
PKGDES="Library for manipulating and displaying accelerating 2D graphics"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DBUILD_ALL=ON \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF"

NOLIBTOOL=0
