PKGNAME=qalculate-tde-trinity
PKGSEC=Trinity
PKGDEP="libqalculate tdelibs"
BUILDDEP="tde-common-cmake"
PKGDES="A Qalculate frontend for the Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DBUILD_ALL=ON \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF"

PKGBREAK="qalculate-trinity"
PKGREP="qalculate-trinity"

NOLIBTOOL=0
