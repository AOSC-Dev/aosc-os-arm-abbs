PKGNAME=kdiff3-trinity
PKGSEC=Trinity
PKGDEP="tdebase tdelibs"
BUILDDEP="tde-common-cmake"
PKGDES="A diff frontend for the Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DBUILD_ALL=ON \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF"

PKGCONFL="kdiff3"

NOLIBTOOL=0
