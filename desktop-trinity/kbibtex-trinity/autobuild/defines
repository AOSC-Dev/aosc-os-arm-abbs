PKGNAME=kbibtex-trinity
PKGSEC=Trinity
PKGDEP="tdelibs yaz"
BUILDDEP="tde-common-cmake"
PKGDES="A BibTeX editor for the Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DBUILD_ALL=ON \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF"

PKGCONFL="kbibtex"

NOLIBTOOL=0
