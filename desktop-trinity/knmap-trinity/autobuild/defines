PKGNAME=knmap-trinity
PKGSEC=Trinity
PKGDEP="nmap tdelibs"
BUILDDEP="tde-common-cmake"
PKGDES="An NMAP frontend for the Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DBUILD_ALL=ON \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF"

PKGCONFL="knmap"

NOLIBTOOL=0
