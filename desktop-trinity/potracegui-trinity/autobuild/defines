PKGNAME=potracegui-trinity
PKGSEC=Trinity
PKGDEP="potrace tdelibs"
BUILDDEP="tde-common-cmake"
PKGDES="A potrace frontend for the Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DBUILD_ALL=ON \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF"

PKGCONFL="potracegui"

NOLIBTOOL=0
