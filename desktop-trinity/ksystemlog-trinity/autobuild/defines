PKGNAME=ksystemlog-trinity
PKGSEC=Trinity
PKGDEP="tdelibs"
BUILDDEP="tde-common-cmake"
PKGDES="A system log viewer for the Trinity Desktop Environment"

CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DBUILD_ALL=ON \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF"

PKGCONFL="ksystemlog"

NOLIBTOOL=0
