abinfo "Stripping out migration tools for fctix5-migrator ..."
mkdir -pv "$SRCDIR"/migratorinstall/usr/{bin,share/applications}
mv -v "$PKGDIR"/usr/bin/fcitx5-migrator \
   "$SRCDIR"/migratorinstall/usr/bin/
mv -v "$PKGDIR"/usr/share/applications/org.fcitx.fcitx5-migrator.desktop \
   "$SRCDIR"/migratorinstall/usr/share/applications/
