PKGNAME=poppler+32
PKGSEC=libs
PKGDES="a PDF rendering library and command line tools used to manipulate PDF files"
PKGDEP="fontconfig+32 glib+32 cairo+32"

AUTOTOOLS_AFTER="--enable-xpdf-headers --enable-cairo-output --disable-libjpeg \
                 --enable-zlib --disable-poppler-qt4 --enable-poppler-glib --disable-poppler-qt5 \
                 --disable-gtk-doc --disable-libcurl --disable-cms --disable-libopenjpeg --disable-gtk-test"
