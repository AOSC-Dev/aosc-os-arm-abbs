abinfo "Setting up CFLAGS and Java version ..."
export EXTRA_CFLAGS="${CPPFLAGS} ${CFLAGS} -Wno-error"
export EXTRA_CPP_FLAGS="${CPPFLAGS} ${CXXFLAGS} -Wno-error -std=gnu++11"
export JAVA_HOME=/usr/lib/java-11/
update-alternatives --set java-home /usr/lib/java-11 ||
    update-alternatives --set java /usr/lib/java-11/bin/java ||
    update-alternatives --set java /usr/lib/java/bin/java || exit 2
