PKGDEP="ca-certs nss xdg-utils hicolor-icon-theme alsa-lib gtk-2 cups \
        fontconfig unzip zip cpio"
BUILDDEP="openjdk-22"
PKGDES="OpenJDK Java Runtime Environment (JRE) and Java Development Environment (JDK) (version 22)"

PKGPROV="jre-openjdk jre${PKGVER%%.*}-openjdk openjdk${PKGVER%%.*} openjdk-${PKGVER%%.*}"
NOPARALLEL=1
PKGEPOCH=3

# FIXME: breaks binary.
NOLTO=1

# Note: No useful symbol can be split out.
ABSPLITDBG=0

# Note: Keep private symbols for development.
