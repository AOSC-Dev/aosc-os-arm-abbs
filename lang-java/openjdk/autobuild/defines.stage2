PKGNAME=openjdk
PKGSEC=java
PKGDEP="ca-certs nss xdg-utils hicolor-icon-theme alsa-lib gtk-2 cups \
        fontconfig unzip zip cpio giflib"
PKGDES="OpenJDK Java Runtime Environment (JRE), Java Development Environment (JDK), and IcedTea-Web"

PKGPROV="icedtea icedtea-web jre-openjdk jre${PKGVER%%.*}-openjdk openjdk${PKGVER%%.*} openjdk-${PKGVER%%.*}"
NOPARALLEL=1
PKGEPOCH=3

# FIXME: breaks binary.
NOLTO=1
ABSPLITDBG=0
