abinfo "Building xcb-proto for Python 2 library"
cd "$SRCDIR"
autoreconf -fvi
./configure ${AUTOTOOLS_DEF}
make
make install BUILDROOT="$PKGDIR" DESTDIR="$PKGDIR"

abinfo "Building xcb-proto for Python 3 library"
./configure ${AUTOTOOLS_DEF} PYTHON=/usr/bin/python3
make
make install BUILDROOT="$PKGDIR" DESTDIR="$PKGDIR"
