cp autobuild/cross-armel/arm-linux.cache config.cache

PYTHON=/usr/bin/python2 ./configure --host=$HOST --prefix=/usr --with-pcre=system --cache-file=config.cache &&
make $ABMK &&
make completiondir=/usr/share/bash-completion/completions DESTDIR=`pwd`/abdist install &&

for _i in "abdist/usr/share/bash-completion/completions/"*; do
    chmod -x "$_i"
done
