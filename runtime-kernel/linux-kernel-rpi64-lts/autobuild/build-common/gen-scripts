cat autobuild/postinst.in | sed 's/@VER@/'"$version"'/g' | sed 's/@LOCAL_VER_RPI4@/'"$LOCALNAME_RPI4"'/g' | sed 's/@LOCAL_VER_RPI5@/'"$LOCALNAME_RPI5"'/g' > autobuild/postinst
cat autobuild/postrm.in | sed 's/@VER@/'"$version"'/g' | sed 's/@LOCAL_VER_RPI4@/'"$LOCALNAME_RPI4"'/g' | sed 's/@LOCAL_VER_RPI5@/'"$LOCALNAME_RPI5"'/g' > autobuild/postrm
