#. autobuild/patch
if [ "$SKIPCONFIG" = "yes" ]
then
true
else
./Configure -des -Dusethreads -Duseshrplib \
    -Dprefix=/usr -Dvendorprefix=/usr \
    -Dman1dir=/usr/share/man/man1 \
    -Dman3dir=/usr/share/man/man3 \
    -Dcccdlflags='${CFLAGS}' \
    -Dlddlflags="-shared ${LDFLAGS}" 
fi
make $ABMK &&
make install DESTDIR=`pwd`/abdist 
