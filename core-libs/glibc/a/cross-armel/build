mkdir build && cd build
cat > config.cache << EOF
libc_cv_forced_unwind=yes
libc_cv_c_cleanup=yes
libc_cv_gnu89_inline=yes
EOF
cat > configparms << EOF
install_root=`pwd`/abdist
EOF
export PATH="`dirname $HOSTTOOLPREFIX`":$PATH
../configure --prefix=/usr --host=$HOST --disable-profile --enable-add-ons --with-tls --enable-kernel=2.6.18 --with-__thread --with-binutils="`dirname $HOSTTOOLPREFIX`" --cache-file=config.cache && make && make install
