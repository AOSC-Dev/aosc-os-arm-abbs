PKGNAME=akode
PKGSEC=Trinity
PKGDEP="arts libmpcdec alsa-lib jack pulseaudio libmad speex libsamplerate flac \
        libvorbis libsndfile"
PKGDEP__RETRO=" \
        arts libmpcdec alsa-lib libmad speex libsamplerate flac \
        libvorbis libsndfile"
PKGDEP__ARMEL="${PKGDEP__RETRO}"
PKGDEP__ARMHF="${PKGDEP__RETRO}"
PKGDEP__I486="${PKGDEP__RETRO}"
PKGDEP__LOONGSON2F="${PKGDEP__RETRO}"
PKGDEP__POWERPC="${PKGDEP__RETRO}"
PKGDEP__PPC64="${PKGDEP__RETRO}"
PKGDES="A decoding library for aRts"

ABTYPE=cmake
CMAKE_AFTER="-DLIB_INSTALL_DIR=/usr/lib \
             -DWITH_ALL_OPTIONS=ON \
             -DWITH_GCC_VISIBILITY=OFF \
             -DWITH_FFMPEG_DECODER=OFF"
CMAKE_AFTER__RETRO=" \
             ${CMAKE_AFTER} \
             -DWITH_JACK_SINK=OFF \
             -DWITH_PULSE_SINK=OFF"
CMAKE_AFTER__ARMEL="${CMAKE_AFTER__RETRO}"
CMAKE_AFTER__ARMHF="${CMAKE_AFTER__RETRO}"
CMAKE_AFTER__I486="${CMAKE_AFTER__RETRO}"
CMAKE_AFTER__LOONGSON2F="${CMAKE_AFTER__RETRO}"
CMAKE_AFTER__POWERPC="${CMAKE_AFTER__RETRO}"
CMAKE_AFTER__PPC64="${CMAKE_AFTER__RETRO}"

NOSTATIC=0

ABUSECMAKEBUILD=0

NOLIBTOOL=0
