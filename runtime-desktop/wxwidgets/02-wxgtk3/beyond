abinfo "Removing unneeded files ..."
rm -v "$PKGDIR"/usr/bin/wxrc*
rm -rv "$PKGDIR"/usr/include
rm -v "$PKGDIR"/usr/lib/libwx_baseu*
rm -rv "$PKGDIR"/usr/share/aclocal
rm -rv "$PKGDIR"/usr/share/bakefile
rm -rv "$PKGDIR"/usr/share/locale

abinfo "Renaming wx-config to wx-config-gtk3 ..."
mv -v "$PKGDIR"/usr/bin/wx-config "$PKGDIR"/usr/bin/wx-config-gtk3
